{
  "name": "es-react-portlet",
  "description": "ES React Portlet",
  "version": "1.0.0",
  "scripts": {
    "rollup": "rollup -c",
    "build": "rollup -c && lnbs-copy-assets && liferay-npm-bundler"
  },
  "portlet": {
    "com.liferay.portlet.display-category": "category.sample",
    "com.liferay.portlet.instanceable": true,
    "javax.portlet.security-role-ref": "power-user,user",
    "javax.portlet.display-name": "ES React Portlet",
    "javax.portlet.name": "es-react-portlet",
    "javax.portlet.resource-bundle": "content.Language"
  },
  "main": "entry",
  "dependencies": {
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^3.1.8",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-replace": "^3.0.0",
    "@rollup/plugin-typescript": "^8.2.1",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.7",
    "esbuild": "^0.12.27",
    "js-tokens": "^7.0.0",
    "liferay-npm-build-support": "^2.28.0",
    "liferay-npm-bundler": "^2.28.0",
    "loose-envify": "^1.4.0",
    "object-assign": "^4.1.1",
    "rollup": "^2.60.1",
    "rollup-plugin-esbuild": "^4.5.0",
    "scheduler": "^0.20.2",
    "typescript": "^4.4.2"
